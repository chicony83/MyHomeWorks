<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DZ4HarvestingAktivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/pshenica" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/harvesting_title"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        style="@style/ConstraintLayoutsInner"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.199"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView_title_company1"
            style="@style/h2_title"
            android:text="@string/company1" />


        <LinearLayout style="@style/agricultures_parent_container">

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        style="@style/ConstraintLayoutsInner"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout4"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView_title_company2"
            style="@style/h2_title"
            android:text="@string/company2" />

        <LinearLayout style="@style/agricultures_parent_container">

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        style="@style/ConstraintLayoutsInner"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        app:layout_constraintVertical_bias="0.822"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView_title_company3"
            style="@style/h2_title"
            android:text="@string/company3" />

        <LinearLayout style="@style/agricultures_parent_container">

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

            <LinearLayout style="@style/agriculture_container">

            </LinearLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>